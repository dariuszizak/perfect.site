    <amp-img src="img/P3057962.JPG" alt="roleta zewnetrzna" width="480px" height="360px"></amp-img>
    <amp-img src="img/P9038860.JPG" alt="roleta zewnetrzna" width="480px" height="360px"></amp-img>
  </amp-carousel>
</article>
{% endblock %}
